{
  "expo": {
    "name": "BBB",
    "description": "A Baby Goods Marketplace.",
    "slug": "BBB",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.16",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "ios": {
      "bundleIdentifier": "com.bebebargains.bbb"
    },
    "android": {
      "package": "com.bebebargains.bbb",
      "versionCode": 17,
      "permissions": [
        "ACCESS_COARSE_LOCATION"
      , "ACCESS_FINE_LOCATION"
      , "CAMERA"
      , "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
